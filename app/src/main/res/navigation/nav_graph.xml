<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/nav_graph"
            app:startDestination="@id/todoFragment">
    <fragment android:id="@+id/todoFragment"
              android:name="com.example.beetlestance.benji.ui.todo.TodoFragment"
              android:label="TodoFragment">
        <action android:id="@+id/action_todoFragment_to_about" app:destination="@id/about"
                app:enterAnim="@anim/nav_default_enter_anim" app:exitAnim="@anim/nav_default_exit_anim"/>
    </fragment>
    <fragment android:id="@+id/about"
              android:name="com.example.beetlestance.benji.ui.about.About"
              android:label="About"
              tools:layout="@layout/about_fragment">
        <action android:id="@+id/action_about_to_todoFragment" app:destination="@id/todoFragment"
                app:enterAnim="@anim/nav_default_enter_anim" app:exitAnim="@anim/nav_default_exit_anim"/>
    </fragment>
</navigation>